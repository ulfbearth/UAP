        </self>
    </action>

    <action id="Foreplay Action">
        <self meter="basic">
            <stat range="0-" statID="WOOF" add=".1"/>
        </self>
        <target meter="basic">
            <stat range="0-" statID="WOOF" add=".1"/>
        </target>
    </action>
    
